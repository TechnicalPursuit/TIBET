{
    "default": {
        "https": false,
        "https_port": 1443,
        "port": 1407,
        "plugins": {
            "tds": ["couch", "tasks"]
        },
        "static": {
            "private": []
        },
        "tasks": {
            "db_name": "{{appname}}_tasks"
        },
        "vcard": {
            "strategy": "tds"
        }
    },
    "development": {
        "log": {
            "transports": ["file", "console"]
        },
        "plugins": {
            "tds": ["cli", "couch", "patch", "tasks", "watch"]
        },
        "use_cli": true
    },
    "production": {
        "https": true
    }
}
