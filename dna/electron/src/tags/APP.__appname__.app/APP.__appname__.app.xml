<?xml version="1.0"?>

<package xmlns="http://www.technicalpursuit.com/1999/tibet"
    basedir="~app_tags/APP.{{appname}}.app"
    name="{{appname}}.app" >

<config id="scripts">
    <!-- tag-related source script references here -->
    <script src="APP.{{appname}}.app.js"/>
</config>

<config id="tests">
    <!-- tag-related test scripts here -->
    <script src="APP.{{appname}}.app_test.js"/>
</config>

<config id="resources">
    <!-- tag-related resource entries here -->
    <resource src="APP.{{appname}}.app.xhtml"/>
    <resource src="APP.{{appname}}.app.css"/>
</config>

</package>
