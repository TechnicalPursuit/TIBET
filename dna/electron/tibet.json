{
    "//" : "TIBET project file. Set configuration parameters here.",
    "project": {
        "name": "{{appname}}"
    },
    "tibet": {
        "dna": "{{dna}}",
        "xmlns": "{{xmlns}}"
    },
    "electron": {
        "//": "NOTE only boot.* params are used on startup URL",
        "boot": {
            "profile": "development"
        },
        "updater": {
            "download": false,
            "onstart": false
        },
        "watch": {
            "event": "fileChange"
        }
    }
}
