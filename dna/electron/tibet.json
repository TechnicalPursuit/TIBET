{
    "//": "TIBET project file. Set configuration parameters here.",
    "project": {
        "name": "{{appname}}"
    },
    "deploy": {
        "electron": {
            "updateURL": "https://{{appname}}.s3.amazonaws.com",
            "provider": "s3",
            "s3": {
                "bucket": "{{appname}}",
                "region": "us-east-1"
            }
        }
    },
    "tibet": {
        "dna": "{{dna}}",
        "xmlns": "{{xmlns}}"
    },
    "electron": {
        "//": "NOTE only boot.* params are used on startup URL",
        "boot": {
            "profile": "development"
        },
        "plugins": {
            "core": ["profile", "window", "menu", "updater", "watch", "devtools"]
        }
    }
}
