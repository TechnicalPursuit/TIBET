<?xml version="1.0"?>

<package xmlns="http://www.technicalpursuit.com/1999/tibet"
    name="phantom" default="phantom" basedir="~lib">

<config id="properties" if="boot.phaseone">
    <property name="log.reporter" value="phantom"/>
    <property name="log.level" value="0"/>
</config>

<config id="phantom">
    <config ref="properties"/>
    <package src="~lib_cfg/TIBET.xml" config="test"/>
</config>

<config id="selftest">
    <config ref="properties"/>
    <package src="~lib_cfg/TIBET.xml" config="developer"/>
    <package src="~lib_cfg/tibet_testing.xml" config="selftests"/>
</config>

</package>
