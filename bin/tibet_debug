var cli,
    version;

cli = require('../src/tibet/cli/_cli.js');

version = process.versions.node;
if (parseInt(version.split('.'), 12) < 12) {
    //  eslint-disable-next-line
    console.error('The TIBET CLI requires Node.js version 12 or higher.');
    return;
}

cli.run({tibet: {}, npm: {}});
