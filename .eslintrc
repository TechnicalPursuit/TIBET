{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "TP": true,
        "APP": true,
        "Window": false,
        "self": false,
        "Proxy": false
    },
    "rules": {
        "array-bracket-spacing": [1, "never"],
        "array-callback-return": 2,
        "brace-style": [2, "1tbs", { "allowSingleLine": false }],
        "camelcase": 0,
        "comma-dangle": 2,
        "comma-spacing": 1,
        "comma-style": 1,
        "computed-property-spacing": [2, "never"],
        "consistent-return": 0,
        "consistent-this": [1, "thisref"],
        "curly": [1, "all"],
        "default-case": 1,
        "dot-location": [1, "object"],
        "dot-notation": 2,
        "eol-last": 0,
        "eqeqeq": 1,
        "func-call-spacing": 1,
        "func-names": [2, never],
        "func-style": 1,
        "guard-for-in": 2,
        "indent": [1, 4, {"SwitchCase": 1, "VariableDeclarator": 1, "outerIIFEBody": 1, "FunctionDeclaration": {"parameters": "first"}, "FunctionExpression": {"parameters": "first"}, "ArrayExpression": 1, "ObjectExpression": 1}],
        "init-declarations": [2, "never"],
        "key-spacing": [1, { "beforeColon": false, "afterColon": true }],
        "keyword-spacing": [2, {"before": true, "after": true, "overrides": {}}],
        "linebreak-style": 2,
        "max-len": [0, 80, 4],
        "max-statements-per-line": 1,
        "new-cap": 0,
        "new-parens": 1,
        "newline-after-var": [2, always],
        "newline-before-return": 0,
        "newline-per-chained-call": 0,
        "no-caller": 2,
        "no-catch-shadow": 2,
        "no-delete-var": 2,
        "no-div-regex": 2,
        "no-else-return": 0,
        "no-empty-function": 1,
        "no-eq-null": 0,
        "no-eval": 2,
        "no-extend-native": 0,
        "no-extra-bind": 2,
        "no-extra-label": 2,
        "no-extra-parens": 1,
        "no-floating-decimal": 1,
        "no-global-assign": 2,
        "no-implied-eval": 1,
        "no-implicit-coercion": [1, {"string": false}],
        "no-invalid-this": 2,
        "no-iterator": 1,
        "no-label-var": 1,
        "no-labels": [2, {"allowLoop": true, "allowSwitch": true}],
        "no-lone-blocks": 1,
        "no-lonely-if": 0,
        "no-loop-func": 2,
        "no-magic-numbers": 0,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-multiple-empty-lines": 1,
        "no-new": 2,
        "no-new-wrappers": 0,
        "no-nested-ternary": 1,
        "no-octal-escape": 2,
        "no-param-reassign": 2,
        "no-process-exit": 2,
        "no-proto": 1,
        "no-return-assign": 1,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 1,
        "no-shadow": 2,
        "no-shadow-restricted-names": 1,
        "no-tabs": 1,
        "no-throw-literal": 2,
        "no-trailing-spaces": 2,
        "no-useless-call": 2,
        "no-undef-init": 2,
        "no-unsafe-negation": 2,
        "no-unused-expressions": 0,
        "no-unused-vars": [1, {"vars": "all", "args": "none"}],
        "no-underscore-dangle": 0,
        "no-use-before-define": 2,
        "no-whitespace-before-property": 2,
        "no-with": 1,
        "object-curly-newline": 0,
        "object-curly-spacing": [1, "never"],
        "one-var": 1,
        "one-var-declaration-per-line": 1,
        "operator-linebreak": [1, "after", { "overrides": { "?": "after"}}],
        "padded-blocks": 0,
        "quotes": [2, "single"],
        "quote-props": [2, "as-needed"],
        "radix": 1,
        "semi": 1,
        "semi-spacing": 1,
        "space-before-blocks": [1, "always"],
        "space-before-function-paren": [1, "never"],
        "space-in-parens": [1, "never"],
        "space-infix-ops": 1,
        "space-unary-ops": [1, { "words": true, "nonwords": false }],
        "spaced-comment": [1, "always"],
        "strict": 0,
        "vars-on-top": 1,
        "wrap-iife": [1, "outside"],
        "yoda": [1, "never"]
    }
}
